<resources>
    <style name="AppThemeStyle" parent="@android:style/Theme.Translucent">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/bg</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowAnimationStyle">@style/SelfDefine.Activity</item>
        <item name="android:scrollingCache">false</item>
    </style>

    <style name="SelfDefine.Activity" parent="@android:style/Animation">
        <item name="android:activityOpenEnterAnimation">@anim/activity_slide_right_in</item>
        <item name="android:activityOpenExitAnimation">@anim/activity_slide_left_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/activity_slide_left_in</item>
        <item name="android:activityCloseExitAnimation">@anim/activity_slide_right_out</item>
    </style>

    <style name="w_w">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="w_m">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="m_m">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="m_w">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowIsFloating">true</item>
        <!-- 设置未浮动窗口 -->
        <item name="android:windowFrame">@null</item>
        <!-- 设置无边框 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 设置无标题 -->
        <item name="android:windowBackground">@color/sc_transparent_background</item>
        <!-- 设置完全透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 设置屏幕变暗 -->
    </style>

    <style name="table_row">
        <item name="android:padding">10dp</item>
    </style>

    <style name="top_bg">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@color/top_bg</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="OperateParentBg">
        <item name="android:layout_marginTop">@dimen/ten_dp</item>
    </style>

    <style name="OperateTvTheme">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/operate_text_size</item>
        <item name="android:layout_marginLeft">@dimen/five_dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">@dimen/five_dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="OperateTvTheme1">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/operate_text_size</item>
    </style>

    <style name="OperateEtTheme">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@color/white</item>
        <item name="android:padding">@dimen/five_dp</item>
        <item name="android:textColorHint">@color/gray</item>
        <item name="android:textSize">@dimen/input_size</item>
    </style>

    <style name="InputParentTheme">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
        <item name="android:padding">@dimen/five_dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="EtTheme">
        <item name="android:textColorHint">@color/et_hint</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item> 
        <item name="android:background">@color/white</item>
        <item name="android:maxLength">11</item>
        <item name="android:textSize">@dimen/input_size</item>
        <item name="android:padding">@dimen/ten_dp</item> 
    </style>

    <style name="BtnCommitTheme">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/bg_round_corner</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">19sp</item>
        <item name="android:padding">@dimen/ten_dp</item>
        <item name="android:layout_marginLeft">@dimen/ten_dp</item>
        <item name="android:layout_marginRight">@dimen/ten_dp</item>
        <item name="android:layout_marginTop">@dimen/fifty_dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="BtnRegisterTheme">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/icon_register_bg</item>
        <item name="android:textColor">@color/text_register_gray</item>
        <item name="android:padding">@dimen/ten_dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">@dimen/one_hundred_eight_dp</item>
    </style>

    <style name="ContentBgTheme">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item> 
        <item name="android:gravity">left</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="btnFogetPwdTheme">
        <item name="android:background">@color/line_gray</item>
        <item name="android:textColor">@color/text_pwd_gray</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="styleOfItemRightText" parent="w_w">
        <item name="android:textSize">@dimen/operate_text_size</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:drawablePadding">5dp</item>
    </style>

    <style name="styleOfTitle" parent="w_w">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/operate_text_size</item>
        <item name="android:paddingRight">@dimen/twenty_dp</item>
    </style>

 
    <style name="styleForTvBottom" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/forty_dp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:background">@color/bg</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">@dimen/ten_dp</item>
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">@dimen/input_size</item>
        <item name="android:visibility">gone</item>
    </style>
    <style name="styleCommonTip" parent="m_w">
        <item name="android:layout_marginLeft">@dimen/twenty_dp</item>
        <item name="android:layout_marginRight">@dimen/twenty_dp</item>
        <item name="android:layout_marginTop">@dimen/forty_dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/input_size</item>
    </style>
    
    <!-- 结交好友验证码 对话框主题 -->
    <style name="common_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>
    <style name="list2LevelText" parent="@style/w_w">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textColor">@color/item_subtitle</item>
        <item name="android:textSize">15sp</item>
    </style>
    
    
    <style name="styleForQuestionRadio" parent="@style/w_w">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="styleForRadioBtnLeft" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@null</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/five_dp</item>
        <item name="android:textSize">@dimen/input_size</item>
    </style>
    
    <style name="styleForRadioBtn" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/five_dp</item>
        <item name="android:textSize">@dimen/input_size</item>
        <item name="android:textColor">@color/text_gray</item>
    </style>
    <style name="styleForListItem" parent="m_w">
        <item name="android:background">@drawable/bg_item_click</item>
        <item name="android:paddingBottom">@dimen/ten_dp</item>
        <item name="android:paddingTop">@dimen/ten_dp</item>
        <item name="android:textColor">@color/text_gray</item>
        <item name="android:textSize">@dimen/input_size</item>
    </style>
    <style name="styleOfLabel" parent="w_w">
        <item name="android:layout_marginBottom">@dimen/five_dp</item>
        <item name="android:layout_marginLeft">@dimen/ten_dp</item>
        <item name="android:layout_marginTop">@dimen/ten_dp</item>
        <item name="android:textColor">@color/top_bg</item>
        <item name="android:textSize">@dimen/input_size</item>
    </style>
    <style name="styleCheckBtn" parent="w_w">
        <item name="android:textColor">@color/text_gray</item>
        <item name="android:textSize">@dimen/input_size</item>
    </style>
</resources>
